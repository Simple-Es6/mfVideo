view{
	box-sizing: border-box;
}
@font-face {
    font-family: 'icomoon';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont{
	font-family: "icomoon" !important;
}
.icon-xin:before {
  content: "\e903";
}
.icon-down:before {
  content: "\e900";
}
.icon-search:before {
  content: "\e901";
}
.icon-up:before {
  content: "\e902";
}
.item{    
    width: 100%;
    height:auto;
    padding-bottom: 10rpx;
}
.item .itemTop{
	width: 100%;
	height:422rpx;
}
.item .itemTop .videoCor{
	position: relative;
	width: 100%;
	height:422rpx;
	background-position: 50% 50%;
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.item .itemTop .videoCor .bgImg{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height:422rpx;
}
.item .itemTop .videoCor .control{
	position: absolute;
	top: 0;
	left: 0;
    z-index:11;
    width:100%;
    height:100%;    
}
.item .itemTop .videoCor .time{
	position: absolute;
	right:30rpx;
	bottom:30rpx;
	z-index:10;
	font-size: 24rpx;
	color: #fff;
}
.item .itemTop .videoCor .logo{
	position: absolute;
	top:30rpx;
	left:30rpx;
	z-index:10;
	width:60rpx;
	height:44rpx;
	opacity: 0;
}
.item .itemTop .videoCor .itemCor{
    position: absolute;
    z-index: 9;
    width:100%;
    height:100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(0,0,0,0);
}
.item .itemTop .videoCor .itemCor .corImg{
	width:90rpx;
	height:90rpx;
}
.item .itemTop .videoCor .itemCon{
	width: 100%;
	height:100%;
}
.item .itemTop .videoCor .itemCon image{
	width: 100%;
	height: 100%;
}
.item .itemTop video{
	width: 100%;
	height:422rpx;
}
.item .itemBot{
	width: 100%;
	height: auto;
	/*padding:20rpx 20rpx 0 20rpx;*/
	padding:13rpx 20rpx;
}
.item .itemBot .title{
	width: 100%;
	height:90rpx;
	color: #1b1b1b;
	font-size: 36rpx;
	line-height:90rpx;
	white-space:nowrap;
	overflow:hidden;
	text-overflow: ellipsis;
}
.item .itemBot .tip{
	display: flex;
	width: 100%;
	height:50rpx;	
}
.item .itemBot .tip .month{	
	height:50rpx;
	line-height:50rpx;	
	flex-grow:1;
	align-self:flex-start;
	color: #909090;
	font-size: 24rpx;
}
.item .itemBot .tip .zan{
	position: relative;
	flex-grow:1;
	align-self:flex-end;
	height:50rpx;
	line-height:50rpx;	
	color: #909090;
}
.item .itemBot .tip .zan .zanTap{
	position: absolute;
	top: 0;
	left: 0;
	z-index: 2;
	width: 100%;
	height: 50rpx;
}
.item .itemBot .tip .zan .like{
	color: #ff5b5a;
}
.item .itemBot .tip .zan .unlike{	
	color: #cccccc;
}
.item .itemBot .tip .zan .iconfont{
	float:right;
	width: auto;
	height:50rpx;
	line-height:50rpx;
	padding-right: 10rpx;
	font-size:32rpx;	
}
.item .itemBot .tip .zan .zanNum{
	float:right;
	width: auto;
	height:50rpx;
	line-height:50rpx;
	padding-right: 10rpx;
	font-size:24rpx;	
}
.item .itemBot .vContent{
	position: relative;
	width: 100%;
	height:auto;	
	padding-top:17rpx;
	padding-bottom:60rpx;
}
.item .itemBot .vContent .con{
	transition:all .3s;
	width: 100%;
	min-height:50rpx;
	line-height:50rpx;
	font-size:30rpx !important;
	color: #666;
	display: -webkit-box;
	word-break: break-all;
	text-overflow: ellipsis;
	overflow: hidden;
	-webkit-box-orient: vertical;
	-webkit-line-clamp:2;
}
.item .itemBot .vContent .kcon{
	transition:all .3s;
	display: table;
}
.item .itemBot .vContent .tapClose{
	position: absolute;
	bottom:0;
	right: 0;
	z-index: 2;
	width:100rpx;
	height: 40rpx;
}
.item .itemBot .vContent .close{
	display: flex;
	position: absolute;
	bottom:0;
	right: 0;
	z-index: 1;
	width:100rpx;
	height: 40rpx;
	color: #909090;
}
.item .itemBot .vContent .close .dong{
	padding-right: 10rpx;
	width: auto;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 24rpx;
	transition:all .3s;
}
.item .itemBot .vContent .close .kai{
	
	transform: rotate(180deg);
}
.item .itemBot .vContent .close .zi{
	width: auto;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 24rpx;
	padding-left: 10rpx;
}
.column{
	width: 100%;
	height: auto;
	padding: 0 40rpx;
}
.citem{
	position: relative;
	width: 100%;
	height:auto;
	padding: 20rpx 0;
}
.citem  .control{
	position: absolute;
	top: 0;
	left: 0;
	z-index:5;
	width:100%;
	height:100%;
}
.citem .citemTop{
	position: relative;
	width: 100%;
	height:300rpx;
	border-radius:16rpx;
	background-position: center;
	background-size:100% 100%;
	background-repeat: no-repeat;
}
.citem .citemTop .videoNum{
	position: absolute;
	right:20rpx;
	bottom:15rpx;
	z-index:3;
	text-align: right;
	width: 200rpx;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
	color: #fff;
}
.citem .citemTop .bg{
	position: absolute;
	left:0;
	top:0;
	z-index:2;
	width: 100%;
	height: 300rpx;
	background: rgba(0,0,0,0);
}
.citem .citemTop .bgImg{
	position: absolute;
	left:0;
	top:0;
	z-index:1;
	width: 100%;
	height: 300rpx;
}
.citem .citemBot{
	width: 100%;
	height:auto;
	padding-top: 20rpx;
}
.citem .citemBot .title{
	width:100%;
	height:auto;
	line-height:70rpx;
	color: #000;
	font-size:36rpx;
}
.citem .citemBot .time{
	width:100%;
	height:60rpx;
	line-height:60rpx;
	color: #A3A3A3;
	font-size:30rpx;
}
page{
	padding-bottom: 30rpx;
}