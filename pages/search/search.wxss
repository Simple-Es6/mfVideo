.container{
	padding-top: 100rpx;
}
.goSearch{
	position: fixed;
	top:0;
	left: 0;
	z-index:66;
	width: 100%;
	height:98rpx;
	padding:22rpx 20rpx;
	background: #fff;	
}
.goSearch .goSearchCon{
	position: relative;
	display: flex;
	/*align-items: center;
    justify-content: center;*/
	width:100%;
	height:54rpx;
	padding: 0 20rpx;
	
}
.goSearch .goSearchCon .quxiao{
	position: absolute;
	top: 0;
	right: 0;
	z-index: 2;
	width: 12%;
	height: 54rpx;
	text-align: right;
	line-height: 54rpx;
	font-size: 24rpx;
	color: #000;
}
.goSearch .goSearchCon .catTap{
	box-sizing: border-box;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 2;
	width:88%;
	height: 54rpx;
	line-height: 54rpx;
	font-size: 24rpx;
	padding:0 20rpx 0 66rpx;
	background: #f9f9f9;
	border-radius:27rpx;
}
.goSearch .goSearchCon .iconfont{
	position: absolute;
	top: 0;
	left:27rpx;
	z-index: 3;
	padding-right:12rpx;
	height: 54rpx;	
	font-size:28rpx;
	line-height:54rpx;
	color:#c7c7c7;
}
.goSearch .goSearchCon .zi{
	height: 54rpx;	
	line-height: 54rpx;
	color:#aeaeae;
	font-size:24rpx;
}
.hisCon{
	width: 100%;
	height: auto;
	
}
.hisCon .hisTitle{
	padding: 0 20rpx;
	width: 100%;
	height:60rpx;
	line-height:60rpx;
	font-size: 24rpx;
	color: #aeaeae;
}
.hisCon .hisItem{
	padding: 0 20rpx;
	width: 100%;
	height:80rpx;
	line-height:80rpx;
	font-size:32rpx;
	color: #000;
	border-bottom: 1rpx 0 #f0f0f0;
}
.hisCon .hisItem:first-of-type{
	border-top: 1rpx 0 #f0f0f0;
}
.hisCon .clearHis{
	width: 100%;
	height:80rpx;
	text-align: center;
	line-height:80rpx;
	font-size:32rpx;
	color: #000;
}
.wuCon{
	margin:100rpx auto;
	width:300rpx;
	height:300rpx;
	background-image: url(../../image/wu.png);
	background-position:center;
	background-repeat: no-repeat;
	background-size:100% auto;
}
